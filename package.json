{
  "name": "cmints-website",
  "version": "0.1.0",
  "description": "Website content for the CMintS project",
  "main": "config.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "cmints --static",
    "deploy": "cmints --static --deploy",
    "start": "cmints --start",
    "dev": "cmints --start --no-cache --dev",
    "crowdin-upload": "cmints --crowdin update-sources --key",
    "crowdin-download": "cmints --crowdin get-translations --key"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Manvel/cmints-website.git"
  },
  "author": "Manvel Saroyan",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Manvel/cmints-website/issues"
  },
  "homepage": "https://github.com/Manvel/cmints-website#readme",
  "dependencies": {
    "cmints": "^0.9.6",
    "highlight.js": "^9.12.0",
    "lang-redirect": "^0.5.5",
    "minimist": "^1.2.0"
  }
}
